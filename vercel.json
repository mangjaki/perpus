{
    "version": 2,
      "framework": null,
    "functions": {
        "api/index.php": { "runtime": "vercel-php@0.6.0" }
    },
    "routes": [
        {
            "src": "/build/(.*)",
            "dest": "/public/build/"
        },
        {
            "src": "/css/(.*)",
            "dest": "/public/css/"
        },
        {
            "src": "/css2/(.*)",
            "dest": "/public/css2/"
        },
        {
            "src": "/images/(.*)",
            "dest": "/public/images/"
        },
        {
            "src": "/images2/(.*)",
            "dest": "/public/images2/"
        },
        {
            "src": "/js/(.*)",
            "dest": "/public/js/"
        },
        {
            "src": "/js2/(.*)",
            "dest": "/public/js2/"
        },
        {
            "src": "/scss/(.*)",
            "dest": "/public/scss/"
        },
        {
            "src": "/fotouser/(.*)",
            "dest": "/public/fotouser/"
        },
        {
            "src": "/fonts2/(.*)",
            "dest": "/public/fonts2/"
        },
        {
            "src": "/videos2/(.*)",
            "dest": "/public/videos2/"
        },
        {
            "src": "/vendors/(.*)",
            "dest": "/public/vendors/"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "true",
        "APP_URL": "https://mdp-vercel.vercel.app/",
 
        "APP_CONFIG_CACHE": "/tmp/config.php",
        "APP_EVENTS_CACHE": "/tmp/events.php",
        "APP_PACKAGES_CACHE": "/tmp/packages.php",
        "APP_ROUTES_CACHE": "/tmp/routes.php",
        "APP_SERVICES_CACHE": "/tmp/services.php",
        "VIEW_COMPILED_PATH": "/tmp",
 
        "CACHE_DRIVER": "array",
        "LOG_CHANNEL": "stderr",
        "SESSION_DRIVER": "cookie"
    }
}   